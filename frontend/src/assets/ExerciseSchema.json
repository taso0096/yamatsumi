{
  "$schema": "http://json-schema.org/draft-07/schema",
  "version": "0.2.1",
  "type": "object",
  "required": [
    "id"
  ],
  "additionalProperties": false,
  "properties": {
    "id": {
      "title": "サイバー空間ID",
      "description": "サイバー空間ID",
      "type": ["integer", "string"],
      "pattern": "^[a-zA-Z0-9_-]+$",
      "examples": [
        "example_network"
      ]
    },
    "scores": {
      "title": "チーム一覧",
      "description": "チーム一覧",
      "type": "object",
      "patternProperties": {
        ".+": {
          "title": "チーム",
          "description": "チーム",
          "type": "object",
          "properties": {
            "score": {
              "title": "チームスコア",
              "description": "チームスコア",
              "type": "integer",
              "examples": [
                100
              ]
            },
            "users": {
              "title": "ユーザ一覧",
              "description": "ユーザ一覧",
              "type": "object",
              "patternProperties": {
                ".+": {
                  "title": "ユーザスコア",
                  "description": "ユーザスコア",
                  "type": ["integer", "null"],
                  "examples": [
                    100
                  ]
                }
              }
            }
          }
        }
      }
    },
    "levels": {
      "title": "難易度一覧",
      "description": "難易度一覧",
      "type": "array",
      "additionalItems": false,
      "items": {
        "type": "object",
        "required": [
          "id"
        ],
        "additionalProperties": false,
        "properties": {
          "id": {
            "title": "難易度ID",
            "description": "難易度ID",
            "type": ["integer", "string"],
            "pattern": "^[a-zA-Z0-9_-]+$",
            "examples": [
              "normal"
            ]
          },
          "label": {
            "title": "難易度表示名",
            "description": "難易度表示名",
            "type": "string",
            "examples": [
              "Normal"
            ]
          },
          "score": {
            "title": "スコア",
            "description": "スコア",
            "type": "integer",
            "examples": [
              100
            ]
          }
        }
      }
    },
    "categories": {
      "title": "カテゴリ一覧",
      "description": "カテゴリ一覧",
      "type": "array",
      "additionalItems": false,
      "items": {
        "type": "object",
        "required": [
          "id"
        ],
        "additionalProperties": false,
        "properties": {
          "id": {
            "title": "カテゴリID",
            "description": "カテゴリID",
            "type": ["integer", "string"],
            "pattern": "^[a-zA-Z0-9_-]+$",
            "examples": [
              "web"
            ]
          },
          "label": {
            "title": "カテゴリ名",
            "description": "カテゴリ名",
            "type": "string",
            "examples": [
              "Web"
            ]
          }
        }
      }
    },
    "questions": {
      "title": "問題一覧",
      "description": "問題一覧",
      "type": "array",
      "additionalItems": false,
      "items": {
        "type": "object",
        "required": [
          "id"
        ],
        "additionalProperties": false,
        "properties": {
          "id": {
            "title": "問題ID",
            "description": "問題ID",
            "type": ["integer", "string"],
            "pattern": "^[a-zA-Z0-9_-]+$",
            "examples": [
              1
            ]
          },
          "levelId": {
            "title": "難易度ID",
            "description": "難易度ID",
            "type": ["integer", "string"],
            "pattern": "^[a-zA-Z0-9_-]+$",
            "examples": [
              "normal"
            ]
          },
          "categoryId": {
            "title": "カテゴリID",
            "description": "カテゴリID",
            "type": ["integer", "string"],
            "pattern": "^[a-zA-Z0-9_-]+$",
            "examples": [
              "web"
            ]
          }
        }
      }
    },
    "routingTable": {
      "title": "ユーザルーティングテーブル",
      "description": "ユーザのルーティングテーブル",
      "type": "object",
      "patternProperties": {
        ".+": {
          "title": "IPアドレス",
          "description": "IPアドレス",
          "type": "array",
          "items": {
            "type": "string",
            "examples": [
              "192.168.1.1"
            ]
          }
        }
      }
    }
  }
}
